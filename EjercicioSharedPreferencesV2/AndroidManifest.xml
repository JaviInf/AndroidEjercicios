<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.javi.sharedpreferences"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.javi.sharedpreferences.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.javi.sharedpreferences.SettinsActivity"
            android:label="@string/title_activity_settins" >
        </activity>
        <activity
            android:name="com.javi.sharedpreferences.PreferenciasActivity"
            android:label="@string/title_activity_preferencias" >
        </activity>
        <activity
            android:name="com.javi.sharedpreferences.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.javi.sharedpreferences.MyPreferenceFragment"
            android:label="@string/title_activity_my_preference_fragment" >
        </activity>
    </application>

</manifest>
